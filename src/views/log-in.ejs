<%- include("template/header", {title,css,token,main}) %>

<main>
  <article>
    <h1 id="title">Vajuli</h1>
    <form>
      <section>
        <input
          class="special-input"
          name="username"
          id="username"
          autocomplete="off"
          type="text"
          required
          autofocus
        />
        <label for="username">Usuario</label>
      </section>
      <section>
        <input name="password" id="password" type="password" required />
        <label for="password">Contrase√±a</label>
        <img src="/icons/show.png" alt="show" id="show-password" class="password-vision" />
        <img
          src="/icons/hide.png"
          alt="hide"
          id="hide-password"
          class="password-vision hide-image"
        />
      </section>
      <input type="submit" value="Ingresar" />
      <input type="button" value="Volver" onclick="location.href = '/'" />
    </form>
  </article>
</main>
<script defer src="/js/log-in.js"></script>

<%- include("template/footer") %>
